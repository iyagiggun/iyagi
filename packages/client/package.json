{
  "name": "@iyagi/client",
  "version": "1.0.0",
  "description": "Client-side renderer for Iyagi using Pixi.js",
  "main": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./shard": {
      "import": "./src/shard/index.js",
      "require": "./src/shard/index.js"
    },
    "./object": {
      "import": "./src/object/index.js",
      "require": "./src/object/index.js"
    },
    "./controller": {
      "import": "./src/controller/index.js",
      "require": "./src/controller/index.js"
    },
    "./const": {
      "import": "./src/const/index.js",
      "require": "./src/const/index.js"
    },
    "./time": {
      "import": "./src/time/index.js",
      "require": "./src/time/index.js"
    },
    "./debug": {
      "import": "./src/debug/index.js",
      "require": "./src/debug/index.js"
    },
    "./effect": {
      "import": "./src/effect/index.js",
      "require": "./src/effect/index.js"
    }
  },
  "type": "module",
  "dependencies": {
    "@iyagi/commons": "file:../commons",
    "@iyagi/server": "file:../server",
    "pixi.js": "^8.16.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "vite": "^5.0.0"
  },
  "license": "ISC"
}
